<div class="page-header">
  <h1>
    Goal times
    <a href="/goal_times/new" class="btn btn-lg btn-success">New Goal time</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Meet</th>
        <th>Goal time</th>
        <th>Actions</th>
      </tr>

      <% @goal_times.each do |goal_time| %>
      <tr>
        <td><%= goal_time.meet.meet_name rescue nil %></td>
        <td><%= goal_time.time.strftime("%I:%M") rescue nil %></td>
        <td>
          <a href="/goal_times/<%= goal_time.id %>" class="btn btn-primary">Show</a>
          <a href="/goal_times/<%= goal_time.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_goal_time/<%= goal_time.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
