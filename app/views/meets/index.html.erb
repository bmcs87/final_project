<div class="page-header">
  <h1>
    Meets
    <a href="/meets/new" class="btn btn-lg btn-success">New Meet</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Meet name</th>
        <th>City</th>
        <th>State</th>
        <th>Date</th>
        <th>Places</th>
        <th>Goal times</th>
        <th>Actual times</th>
        <th>Athlete notes</th>
        <!-- <th>Users</th>
        <th>Actions</th> -->
      </tr>

      <% @meets.each do |meet| %>
      <tr>
        <td><%= meet.meet_name %></td>
        <td><%= meet.city %></td>
        <td><%= meet.state %></td>
        <td><%= meet.date %></td>
        <td><%= meet.goal_times_id %></td>
        <td><%= meet.actual_times_id %></td>
        <td><%= meet.places_id %></td>
        <td><%= meet.users_id %></td>
        <td><%= meet.athlete_notes %></td>
        <td>
          <a href="/meets/<%= meet.id %>" class="btn btn-primary">Show</a>
          <a href="/meets/<%= meet.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_meet/<%= meet.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
